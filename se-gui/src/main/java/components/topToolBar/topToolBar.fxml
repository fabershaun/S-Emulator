<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefWidth="Infinity" spacing="8.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.topToolBar.TopToolBarController">
   <ComboBox fx:id="programSelectorCB" promptText="Program \ Function Selector">
      <HBox.margin>
         <Insets right="7.0" />
      </HBox.margin>
   </ComboBox>
   <ComboBox fx:id="collapseCB" prefWidth="90.0" promptText="Collapse" />
    <VBox alignment="CENTER" prefWidth="90.0">
        <Label text="Current Degree" />
        <Label fx:id="currentDegreeLabel" layoutX="10.0" layoutY="10.0" />
    </VBox>
   <ComboBox fx:id="expandCB" prefWidth="90.0" promptText="Expand" />
    <ComboBox fx:id="highlightSelectionCB" prefWidth="150.0" promptText="Highlight Selection">
        <HBox.margin>
          <Insets left="7.0" />
        </HBox.margin>
    </ComboBox>
</HBox>
